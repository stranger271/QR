<script type="text/javascript">

    $(document).ready(function () {
        bsCustomFileInput.init()
    })
</script>

<script src="~/Scripts/Upload.js" type="text/javascript">
</script>


<nav class="navbar sticky-top navbar-dark bg-dark  mx-n3 mb-1 ">
    <div class="navbar-brand">Обновление расписания</div>
</nav>









@using (Html.BeginForm("Upload", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <label for="password">Волшебное слово</label>
        <input type="password" class="form-control" id="password" name="password">
    </div>

    <div class="custom-file w-50">
        <input type="file" class="custom-file-input" id="customFile" accept="application/pdf" name="file">
        <label class="custom-file-label" for="customFile">Выберите файл с расписанием</label>
    </div>


    <input type="submit" value="Загрузить" class="btn btn-success" />


}



